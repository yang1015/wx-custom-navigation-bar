.nav-wrapper {
    width: 100%;
    box-sizing: border-box;
    position: fixed;
    display: flex;
    align-items: center;
}

.previous-icon {
    width: 40rpx;
    height: 40rpx;
    padding: 0 33rpx;
    z-index: 100;
}

.nav-title {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    box-sizing: border-box;
    z-index:10;
}

.text-size-nav {
    font-size: 34rpx;
}
